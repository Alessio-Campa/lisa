class expressions {
	assignment() {
		def i = 5;
		def x = "s";
	}
	
	_throw(x, i) {
		throw x;
		throw i;
	}

	_return(x, i) {
		return i;
		return x;
	}

	call(i) {
		this.test(5);
		this.test(i, 5);
	}

	nestedCall(i, x) {
		this.test(5, this.test(i, 5));
		this.test(x, this.test(i));
		return this.test(i);
		throw this.test(i);
		x = this.test(i);
	}
}